    <section id="contact">
        <div class="container-fluid">

            <header class="text-center mb-60">
                <h2>Contact Us</h2>
                <hr />
            </header>

            <div class="row">
                <!-- FORM -->
                <div class="col-md-8 col-sm-8">

                    <h3>Send us your project ideas!!</h3>

                    <!-- Alert Success -->
                    <div id="alert_success" class="alert alert-success mb-30">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <strong>Thank You!</strong> Your message successfully sent!
                    </div><!-- /Alert Success -->


                    <!-- Alert Failed -->
                    <div id="alert_failed" class="alert alert-danger mb-30">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <strong>[SMTP] Error!</strong> Internal server error!
                    </div><!-- /Alert Failed -->


                    <!-- Alert Mandatory -->
                    <div id="alert_mandatory" class="alert alert-danger mb-30">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        <strong>Sorry!</strong> You need to complete all mandatory (*) fields!
                    </div><!-- /Alert Mandatory -->

                    <fieldset>
                        <input type="hidden" name="action" value="contact_send" />

                        <div class="row">
                            <div class="col-md-4">
                                <label for="leads_full_name">Full Name *</label>
                                <input required type="text" value="" class="form-control" name="lead[full_name]" id="leads_full_name">
                            </div>
                            <div class="col-md-4">
                                <label for="leads_email">E-mail Address *</label>
                                <input required type="email" value="" class="form-control" name="lead[email]" id="leads_email">
                            </div>
                            <div class="col-md-4">
                                <label for="leads_phone">Phone</label>
                                <input type="text" value="" class="form-control" name="lead[phone]" id="leads_phone">
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-4">
                                <label for="leads_company_name">Business Name</label>
                                <input type="text" value="" class="form-control" name="lead[company_name]" id="leads_company_name">
                            </div>
                            <div class="col-md-4">
                                <label for="leads_project_name">Project Name</label>
                                <input type="text" value="" class="form-control" name="lead[project_name]" id="leads_project_name">
                            </div>
                            <div class="col-md-4">
                                <label for="leads_department">Department *</label>
                                <select required class="form-control pointer" name="lead[department]" id="leads_department">
                                    <option value="">--- Select ---</option>
                                    <option value="Question">Question</option>
                                    <option value="Residential">Residential</option>
                                    <option value="Corporate">Corporate</option>
                                </select>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <label for="leads_description">Project Description</label>
                                <input type="text" value="" class="form-control" name="lead[project_desc]" id="leads_description">
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <label for="leads_message">Message *</label>
                                <textarea required maxlength="10000" rows="8" class="form-control" name="lead[message]" id="leads_message"></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <label for="leads_attachment">File Attachment</label>

                                <!-- custom file upload -->
                                <input class="custom-file-upload" type="file" id="file" name="lead[attached_file]" id="leads_attachment" data-btn-text="Select a File" />
                                <small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png)</small>

                            </div>
                        </div>
                        <%= recaptcha_tags %>

                    </fieldset>

                    <div class="row">
                        <div class="col-md-12">
